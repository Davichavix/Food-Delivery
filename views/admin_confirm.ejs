<!DOCTYPE html>
<html lang="en">

<head>
  <title>Confirm Order</title>

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>

  <!-- App JS -->
  <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

  <script type="text/javascript" src="/scripts/admin_confirm.js" type="text/javascript"></script>
  <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
  <script src="https://code.jquery.com/ui/1.13.1/jquery-ui.js"></script>


  <!-- CSS -->
  <link rel="stylesheet" href="/styles/header.css" type="text/css" />
  <link rel="stylesheet" href="/styles/admin_confirm.css" type="text/css" />


  <!-- Google Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@700&family=Source+Sans+Pro:wght@300;400;600&display=swap" rel="stylesheet">
</head>

<body>
  <%- include('partials/admin_header') %>
  <main>
    <h1 class='title'></h1>

    <div class="table-content">
      <div class="confirm-table">
        <form class="confirm-form">
          <label for="lang">Will be ready in: </label>
          <select name="time" id="lang">
            <option value="" disabled selected>Select a time</option>
            <option value="15">15 min</option>
            <option value="30">30 min</option>
            <option value="45">45 min</option>
            <option value="60">1 hr</option>
            <option value="75">1 hr 15 min</option>
            <option value="90">1 hr 30 min</option>
            <option value="105">1 hr 45 min</option>
            <option value="120">2 hr</option>
          </select>
        </form>
      </div>

      <div class="order-table">
      </div>

      <button type="button" class="btn-trans cancel-button">Cancel</button>
      <button type="button" class="btn confirm-button">Confirm</button>

    </div>
  </main>

</body>

</html>
